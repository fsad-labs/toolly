{
  "name": "@fsad-labs/toolly",
  "author": "fullstack-ad",
  "version": "1.0.0",
  "description": "Tools functions",
  "keywords": [
    "tool",
    "tools",
    "utils",
    "utility",
    "functions",
    "retry"
  ],
  "scripts": {
    "build": "tsup ./index.ts --format esm,cjs --dts --out-dir dist",
    "dev": "tsup ./index.ts --watch",
    "start": "npx tsc index.ts && node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fsad-labs/toolly.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}